{
    "name": "quiqqer\/countries",
    "type": "quiqqer-plugin",
    "description": "Extend QUIQQER with a country database",
    "license": "MIT",
    "authors": [
        {
            "name": "Henning Leutz",
            "email": "leutz@pcsg.de",
            "homepage": "http:\/\/www.pcsg.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@pcsg.de"
    },
    "require": {
        "quiqqer\/quiqqer": "*",
        "quiqqer\/utils": "*@dev",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.3",
        "rector/rector": "^0.17.6",
        "phpstan/phpstan": "^1.11",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-mockery": "^1.1"
    },
    "autoload": {
        "psr-0": {
            "QUI": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QUI\\Countries\\": "tests/QUI/Countries"
        }
    },
    "repositories": {
        "qassets": {
            "type": "composer",
            "url": "https://assets.quiqqer.com"
        },
        "qcomposer": {
            "type": "composer",
            "url": "https://composer.quiqqer.com"
        },
        "qupdate": {
            "type": "composer",
            "url": "https://update.quiqqer.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "phpstan/extension-installer": true
        }
    }
}
